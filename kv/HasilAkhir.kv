#:import MDToolbar kivymd.uix.toolbar.MDToolbar
#:import utils kivy.utils

<HasilAkhir>
    name: "hasilakhir"
    # GridLayout:
    #     cols: 1
    BoxLayout:
        pos_hint: {"center_x": 0.5, "center_y": 0.73}
        padding: 0,50,0,0
        size_hint_y: None
        height: 300
        width: 350
        id: box
    BoxLayout:
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        height:500
        width:300
        orientation: "vertical"
        padding: 40, 150, 40, 50
        spacing: 30
        BoxLayout:
            orientation: "vertical"
            size_hint_y: None
            height: 200
            padding: 0,0,0,0
            # spacing: 30
            halign: "center"
            BoxLayout:
                orientation: "horizontal"
                MDLabel:
                    text: "Tanggal Panen"
                MDLabel:
                    id: hasilakhir_date
                    halign: "right"
            BoxLayout:
                orientation: "horizontal"
                MDLabel:
                    text: "Blok"
                MDLabel:
                    id: hasilakhir_blok
                    halign: "right"
            BoxLayout:
                orientation: "horizontal"
                MDLabel:
                    text: "Varietas"
                MDLabel:
                    id: hasilakhir_varietas
                    halign: "right"
            BoxLayout:
                orientation: "horizontal"
                MDLabel:
                    text: "Proses"
                MDLabel:
                    id: hasilakhir_proses
                    halign: "right"
            BoxLayout:
                orientation: "horizontal"
                MDLabel:
                    text: "Biaya/Kg (Total)"
                MDLabel:
                    id: hasilakhir_biaya
                    halign: "right"

        MDRaisedButton:
            text: "Selesai"
            pos_hint: {"center_x": 0.8, "center_y": 0.1}
            font_size: 20
            halign: "center"
            specific_text_color: 1, 1, 1, 1
            on_release:
                app.cleargraph()
                app.root.ids.screen_manager.current = "dashboard"
                root.manager.transition.direction = "right"
