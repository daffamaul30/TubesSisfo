#:import utils kivy.utils
# :include kv/Main.kv

<Content>:
    id: listproduk
    on_release: app.root.ids.screen_manager.current = "hasilakhir"
<Dashboard>
    name: "dashboard"
    BoxLayout:
        orientation: "vertical"
        padding: 0,65,0,0
        MDScrollViewRefreshLayout:
            id: refresh_layout
            refresh_callback: app.refresh_callback
            root_layout: root
            GridLayout:
                cols: 1
                size_hint_y: None
                height: self.minimum_height
                id: box