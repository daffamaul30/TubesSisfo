<Content>:
    on_release: 
        #app.root.ids.screen_manager.current = "hasilakhir"
        
        app.test_toast(self.id)
    IconRightWidget:
        icon: "delete"
        pos_hint: {"center_x": 0.9, "center_y": 0.5}
        on_release: app.delete(root.id)
<Dashboard>
    name: "dashboard"
    BoxLayout:
        orientation: "vertical"
        padding: 0,65,0,0
        MDScrollViewRefreshLayout:
            id: refresh_layout
            refresh_callback: app.refresh_callback
            root_layout: root
            GridLayout:
                cols: 1
                size_hint_y: None
                height: self.minimum_height
                id: box